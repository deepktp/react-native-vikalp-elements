{
  "extends": "@react-native",
  "rules": {
    "react-native/no-inline-styles": 0,
    "@typescript-eslint/no-unused-vars": [
      "error",
      { "ignoreRestSiblings": true }
    ],
    "no-console": ["error", { "allow": ["warn", "error"] }]
  },
  "parserOptions": {
    "parser": "@babel/eslint-parser",
    "requireConfigFile": false
  }
}
